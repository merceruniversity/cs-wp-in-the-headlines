{
  "name": "cs-wp-in-the-headlines",
  "version": "1.0.0",
  "description": "CommonSpot element for In-The-Headlines-style news feeds.",
  "main": "index.js",
  "scripts": {
    "prebuild": "run-s clean",
    "build:js": "browserify src/js/late-loading.js -o dist/late-loading.js -t [ babelify --presets [ latest ] ]",
    "build:css:element:node-sass": "node-sass src/scss/early-loading.scss dist/early-loading.css",
    "build:css:element:postcss": "postcss dist/early-loading.css -u autoprefixer -o dist/early-loading.css",
    "build:css:element": "run-s build:css:element:*",
    "build:css:test:node-sass": "node-sass src/scss/test.scss test/resources/test/test.css",
    "build:css:test:postcss": "postcss test/resources/test/test.css -u autoprefixer -o test/resources/test/test.css",
    "build:css:test": "run-s build:css:test:*",
    "build:css": "run-p build:css:*",
    "build": "run-p build:*",
    "postbuild:bootstrap:mkdirp": "mkdirp test/resources/bootstrap/",
    "postbuild:bootstrap:ncp": "ncp node_modules/bootstrap/dist/ test/resources/bootstrap/",
    "postbuild:bootstrap": "run-s postbuild:bootstrap:*",
    "postbuild:in-the-news:mkdirp": "mkdirp test/resources/in-the-news/",
    "postbuild:in-the-news:ncp": "ncp dist/ test/resources/in-the-news/",
    "postbuild:in-the-news": "run-s postbuild:in-the-news:*",
    "postbuild:jquery:mkdirp": "mkdirp test/resources/jquery/",
    "postbuild:jquery:ncp": "ncp node_modules/jquery/dist/ test/resources/jquery/",
    "postbuild:jquery": "run-s postbuild:jquery:*",
    "postbuild:swiper:mkdirp": "mkdirp test/resources/swiper/",
    "postbuild:swiper:ncp": "ncp node_modules/swiper/dist/ test/resources/swiper/",
    "postbuild:swiper": "run-s postbuild:swiper:*",
    "postbuild": "run-p postbuild:*",
    "clean:test-resources": "rimraf test/resources && mkdirp test/resources",
    "clean:dist": "rimraf dist &&  mkdirp dist",
    "clean": "run-p clean:*",
    "test": "live-server test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/merceruniversity/cs-wp-in-the-headlines.git"
  },
  "keywords": [
    "CommonSpot",
    "element",
    "WordPress",
    "JavaScript"
  ],
  "author": "Todd Sayre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/merceruniversity/cs-wp-in-the-headlines/issues"
  },
  "homepage": "https://github.com/merceruniversity/cs-wp-in-the-headlines#readme",
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "babel-preset-latest": "^6.24.1",
    "babelify": "^8.0.0",
    "babili": "^0.1.4",
    "bootstrap": "^3.3.7",
    "browserify": "^16.1.1",
    "cpy-cli": "^1.0.1",
    "cssnano-cli": "^1.0.5",
    "fetch-jsonp": "^1.1.3",
    "flickity": "^2.0.11",
    "jquery": "^3.3.1",
    "jshint": "^2.9.5",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.8.1",
    "npm-run-all": "^4.1.2",
    "object-get": "^2.1.0",
    "object-has": "^1.0.2",
    "postcss-cli": "^5.0.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "sass-bem": "^2.6.5",
    "swiper": "^4.1.6"
  }
}
